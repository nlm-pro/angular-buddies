{
  "name": "@mace/prettier-schematics",
  "version": "0.4.1",
  "description": "Add Prettier to your Angular CLI projects.",
  "scripts": {
    "preversion": "npm run test",
    "version": "standard-changelog && git add CHANGELOG.md",
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine **/*_spec.js",
    "prettify": "prettier --write ./**/*.{json,css,scss,md,js,ts}"
  },
  "keywords": [
    "schematics"
  ],
  "author": "Noël Macé <contact@noelmace.com>",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^7.0.3",
    "@angular-devkit/schematics": "^7.0.3",
    "@types/jasmine": "^2.6.0",
    "@types/node": "^10.12.0",
    "jasmine": "^3.2.0",
    "typescript": "^3.1.3"
  },
  "devDependencies": {
    "@schematics/angular": "^7.0.3",
    "prettier": "^1.12.1",
    "standard-changelog": "^2.0.1"
  }
}
